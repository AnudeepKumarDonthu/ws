/**
 * @author Anudeep Kumar
 *
 */
public class ExceptionTest {

	/**
	 * @param args
	 */
	void method(int...k){
		for(int i:k){
			System.out.print(i);
		}
	}
	public static void main(String[] args) {
		ExceptionTest e = new ExceptionTest();
		e.method(1,2,3);
		e.method(1,2,3,4);
		e.method(1,2,3,4,5);
	}

}

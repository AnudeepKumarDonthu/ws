<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>JSON Array Pactice</title>
</head>
<body>
	<script type="text/javascript">
		var personArray = {
			"uma" : [ {
				"umafullname" : "Akurathi Uma Maheshwari",
				"umaage" : 24
			}, {
				"umafullname" : "Akurathi Uma ",
				"umaage" : 23
			} ],
			"anudeep" : [ {
				"andeepfullname" : "Donthu Anudeep Kumar",
				"anuage" : 25
			}, {
				"andeepfullname" : "Anudeep Kumar",
				"anuage" : 25
			} ]

		}

		var i = 0
		document.writeln("<table border = '2'><tr>");

		for (i = 0; i < personArray.uma.length; i++) {
			document.writeln("<td>");
			document.writeln("<table border = '1' width = 100 >");
			document.writeln("<tr><td><b>Name</b></td><td width = 50>"
					+ personArray.uma[i].umafullname + "</td></tr>");
			document.writeln("<tr><td><b>Price</b></td><td width = 50>"
					+ personArray.uma[i].umaage + "</td></tr>");
			document.writeln("</table>");
			document.writeln("</td>");
		}

		for (i = 0; i < personArray.anudeep.length; i++) {
			document.writeln("<td>");
			document.writeln("<table border = '1' width = 100 >");
			document.writeln("<tr><td><b>Name</b></td><td width = 50>"
					+ personArray.anudeep[i].andeepfullname + "</td></tr>");
			document.writeln("<tr><td><b>Price</b></td><td width = 50>"
					+ personArray.anudeep[i].anuage + "</td></tr>");
			document.writeln("</table>");
			document.writeln("</td>");
		}

		document.writeln("</tr></table>");
	</script>
</body>
</html>
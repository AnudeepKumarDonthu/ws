package co.test;

/**
 * @author Anudeep Kumar
 *
 */
public class StaticTest {

	/**
	 * @param args
	 */
	static int i = 0;
	static {
		System.out.println(i);
	}

	public void Test() {
		System.out.println(i);
	}
	public static void staticTest() {
		System.out.println(i);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(i);
	}

}
